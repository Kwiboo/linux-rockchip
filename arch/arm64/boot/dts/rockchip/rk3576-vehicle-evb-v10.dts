&gmac1 {
	status = "disabled";
};

&hym8563 {
	status = "disabled";
};

&pinctrl {
	touch {
		//dsi-i2c8
		touch_gpio_dsi: touch-gpio-dsi {
			rockchip,pins =
				<3 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
		//dp-i2c5
		touch_gpio_dp: touch-gpio-dp {
			rockchip,pins = <2 RK_PD3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
		//edp0-i2c2
		touch_gpio_edp: touch-gpio-edp {
			rockchip,pins =
				<4 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	vcc5v0-buck {
		vcc5v0_buck_en: vcc5v0-buck-en {
			rockchip,pins = <0 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

};

&sdmmc {
	status = "disabled";
};

&u2phy0_otg {
	phy-supply = <&vcc5v0_host_usb20>;
};

&u2phy1_otg {
	phy-supply = <&vcc5v0_host_usb30>;
	status = "okay";
};

&usb_drd0_dwc3 {
	dr_mode = "peripheral";
	maximum-speed = "high-speed";
	extcon = <&u2phy0>;
	status = "okay";
};

&usb_drd1_dwc3 {
	dr_mode = "host";
	status = "okay";
};
